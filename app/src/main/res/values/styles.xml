<resources>

    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@android:color/holo_blue_bright</item>
        <item name="colorPrimaryDark">@android:color/holo_blue_light</item>
        <item name="colorAccent">@android:color/holo_blue_dark</item>
    </style>

    <style name="Base.Theme.WanAndroid" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@android:color/holo_blue_bright</item>
        <item name="colorOnPrimary">@android:color/holo_blue_bright</item>
        <item name="colorOnSurface">@android:color/holo_blue_bright</item>
        <item name="colorPrimaryDark">@android:color/holo_blue_light</item>
        <item name="android:colorBackground">@android:color/white</item>
    </style>

    <style name="Theme.WanAndroid" parent="Base.Theme.WanAndroid" />

    <style name="Theme.WanAndroid.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="line">
        <item name="android:background">#EEEFF3</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>
</resources>
